{
  "name": "@create-vite-plugin-ssr/boilerplate",
  "version": "0.1.0-beta.1",
  "description": "UI package for scaffolding @create-vite-plugin-ssr boilerplates",
  "private": false,
  "typings": "./typings",
  "dependencies": {
    "@create-vite-plugin-ssr/typings": "^0.1.0-beta.1"
  },
  "scripts": {
    "format:check": "prettier --check \"**/*.{js,ts,tsx,vue,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,vue,json}\"",
    "dev": "echo Specify a UI [preact, react, vue]: dev:ui",
    "dev:preact": "cross-env VITE_APP_FRAMEWORK=Preact ts-node --project tsconfig.preact.json ./server",
    "dev:react": "cross-env VITE_APP_FRAMEWORK=React ts-node --project tsconfig.react.json ./server",
    "dev:vue": "cross-env VITE_APP_FRAMEWORK=Vue ts-node --project tsconfig.vue.json ./server",
    "//prod": "npm run build && npm run server:prod",
    "prod": "echo Production is disabled for the boilerplate.",
    "//build": "vite build && vite build --ssr",
    "build": "echo Build is disabled for the boilerplate.",
    "//server": "ts-node ./server",
    "//server:prod": "cross-env NODE_ENV=production ts-node ./server"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@preact/preset-vite": "^2.2.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@vitejs/plugin-react": "^1.3.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "vite": "^2.9.5",
    "vite-plugin-ssr": "^0.3.64",
    "vue": "^3.2.33"
  },
  "license": "MIT"
}
