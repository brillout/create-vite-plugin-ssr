{
  "name": "create-vite-plugin-ssr",
  "version": "0.0.150",
  "repository": "https://github.com/brillout/vite-plugin-ssr",
  "license": "MIT",
  "keywords": [
    "vite",
    "vite-plugin",
    "ssr"
  ],
  "engines": {
    "node": ">=12.19.0"
  },
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "create-vite-plugin-ssr": "./dist/index.js",
    "update-dependencies": "./dist/update/index.js"
  },
  "scripts": {
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,json}\"",
    "clean": "rm -r dist",
    "dev:update": "ts-node --files src/update/index.tsx",
    "dev:create": "ts-node --files src/index.tsx",
    "build:update": "tsc && node dist/update/index.js",
    "build:create": "tsc && node dist/index.js"
  },
  "devDependencies": {
    "@badisi/latest-version": "^2.1.3",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "figures": "^3.2.0",
    "ink": "^3.2.0",
    "ink-select-input": "^4.2.1",
    "ink-text-input": "^4.0.3",
    "prettier": "^2.6.2",
    "react": "^17.0.2"
  }
}
